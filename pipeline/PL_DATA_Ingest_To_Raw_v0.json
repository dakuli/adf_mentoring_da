				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "ACT_GEN_META_GetFilesList",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "ListOfFiles",
					"value": {
						"value": "@activity('ACT_GEN_META_GetFilesList').output.childItems",
						"type": "Expression"
					}
				}
			}
		],
		"variables": {
			"filename": {
				"type": "String"
			},
			"current_timestamp": {
				"type": "String"
			},
			"ListOfFiles": {
				"type": "Array"
			},
			"OutputFIleName": {
				"type": "String"
			}
		},
		"folder": {
			"name": "archive"
		},
		"annotations": [],
		"lastPublishTime": "2024-05-03T21:52:01Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
