							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "OutputFIleName",
								"value": {
									"value": "@concat(replace(item().name,'.csv','_'),\nvariables('current_timestamp'),'.csv')",
									"type": "Expression"
								}
							}
						}
					]
				}
			},
			{
				"name": "ACT_GEN_SETVAR_CurrentDatetime",
				"type": "SetVariable",
				"dependsOn": [],
