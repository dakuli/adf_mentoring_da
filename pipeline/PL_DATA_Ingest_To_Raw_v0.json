					{
						"activity": "ACT_GEN_SETVAR_ListOfFiles",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('ListOfFiles')",
						"type": "Expression"
					},
					"batchCount": 1,
					"activities": [
						{
							"name": "ACT_MT_Copy_FileIngestToRaw",
							"type": "Copy",
							"dependsOn": [
								{
									"activity": "ACT_GEN_SETVAR_OutputFIleName",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
